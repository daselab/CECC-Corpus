<?xml version="1.0" encoding="GB2312"?>
<Body>
    <Title>云南盈江发生5.0级地震</Title>
    <ReportTime type="absTime">2008-8-20</ReportTime>
    <Content>
        <Paragraph>
            <Sentence>
                <Event eid="e1" type="thoughtevent"> 
                    <Participant sid="s1">新华网昆明</Participant>
                    <Time tid="t1" type="relTime">8月20日</Time> 
                    <Denoter did="d1" type="statement">电</Denoter>
                </Event>（记者蔡祥荣）
                <Event eid="e2">
                    <Location lid="l2">云南省盈江县</Location>
                    <Time type="relTime" tid="t2">20日早晨</Time>发生
                    <Denoter type="emergency" did="d2">地震</Denoter>后
                </Event>，
                <Event eid="e3">
                    <Participant sid="s3">云南省地震局</Participant>立即
                    <Denoter type="operation" did="d3">派出</Denoter>
                    <Participant oid="o3" sid="s4,s5">30名地震专家</Participant>
                </Event>，
                <Event eid="e4">于
                    <Time type="relTime" tid="t4">当日7时许</Time>从
                    <Location type="origin" lid="l4">昆明</Location>出发，
                    <Denoter type="movement" did="d4">赶赴</Denoter>
                    <Location type="destination" lid="l4">盈江震区</Location>
                </Event>
                <Event eid="e5">
                    <Denoter type="operation" did="d5">指挥</Denoter>
                    <Object oid="o5">救援</Object>
                </Event>。
            </Sentence>
        </Paragraph>
        <Paragraph>
            <Sentence>
                <Event eid="e6">
                    <Time type="absTime" tid="t6">北京时间8月20日5时35分</Time>，
                    <Location lid="l6">云南省德宏傣族景颇族自治州盈江县</Location>发生5．0级
                    <Denoter type="emergency" did="d6">地震</Denoter>，震中位于北纬25．1度、东经97．9度
                </Event>。
            </Sentence>
        </Paragraph>
    </Content>
    <eRelation relType="Causal" cause_eid="e2" effect_eid="e3"/>
    <eRelation relType="Thoughtcontent" thoughtevent_eid="e1" thoughtcontent_eids="e2-e6"/>
    <eRelation relType="Follow" bevent_eid="e2" aevent_eid="e3"/>
    <eRelation relType="Accompany" accompanya_eid="e3" accompanyb_eid="e4"/>
    <eRelation relType="Follow" bevent_eid="e4" aevent_eid="e5"/>
    
    <eAnaphora anaType="Location" aid="l2" antecedent="云南省盈江县" rid="l4" anaphor="盈江震区"/>
    <eAnaphora anaType="Location" aid="l4" antecedent="盈江震区" rid="l6" anaphor="云南省德宏傣族景颇族自治州盈江县"/>
    
    <eAnaphora anaType="Time" aid="t1" antecedent="8月20日" rid="t4" anaphor="当日7时许"/>
    
    <eAnaphora anaType="Event" aid="e2" rid="e6"/>
