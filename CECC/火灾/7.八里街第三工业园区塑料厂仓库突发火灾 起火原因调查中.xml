<?xml version="1.0" encoding="GB2312" ?>
<Body>
    <Title>八里街第三工业园区塑料厂仓库突发火灾 起火原因调查中</Title>
    <ReportTime type="absTime">2014年03月31日 11:22:07 星期一</ReportTime>
    <Content>
        <Paragraph> 
            <Sentence>
                <Event eid="e1">
                    <Time type="relTime" tid="t1">29日下午4时许</Time>，
                    <Location lid="l1">八里街第三工业园区里的一间塑料厂成品仓库</Location>突发
                    <Denoter type="emergency" did="d1">火灾</Denoter>
                </Event>，
                <Event eid="e2">
                    <Object sid="s2,s3">大量塑料编织袋</Object>
                    <Denoter type="emergency" did="d2">起火</Denoter>
                </Event>，
                <Event eid="e3">
                    <Denoter type="action" did="d3">冒起</Denoter>
                    <Object oid="o3">滚滚浓烟</Object>
                </Event>。
            </Sentence>
            <Sentence>
                <Event eid="e4">
                    <Participant sid="s4,s5,s6,s7">消防</Participant>、
                    <Participant sid="s4,s5,s6,s7">民警</Participant>相继
                    <Denoter type="movement" did="d4">赶到</Denoter>
                </Event>，
                <Event eid="e5">一边
                    <Denoter type="operation" did="d5">疏散</Denoter>
                    <Participant oid="o5">群众</Participant>
                </Event>，
                <Event eid="e6">一边加紧
                    <Denoter type="operation" did="d6">救火</Denoter>
                </Event>，
                <Event eid="e7">
                    <Time type="relTime" tid="t7">到当晚9时许</Time>，
                    <Object oid="o7">火势</Object>已被
                    <Denoter type="operation" did="d7">控制</Denoter>
                </Event>，
                <Event eid="e8">所幸
                    <Denoter type="emergency" did="d8">大火</Denoter>
                </Event>
                <Event eid="e9" polarity="negative">未造成
                    <Participant sid="s9">人员</Participant>
                    <Denoter type="stateChange" did="d9">伤亡</Denoter>
                </Event>。
            </Sentence>
            <Sentence>
                <Event eid="e10">
                    <Time type="relTime" tid="t10">截至30日晚</Time>
                    <Participant sid="s10">记者</Participant>
                    <Denoter type="operation" did="d10">发稿</Denoter>时
                </Event>，
                <Event eid="e11">
                    <Object oid="o11">起火原因</Object>尚在
                    <Denoter type="operation" did="d11">调查</Denoter>中
                </Event>。
            </Sentence>
        </Paragraph>
    </Content>
    <eRelation relType="Causal" cause_eid="e1" effect_eid="e2"/>
    <eRelation relType="Accompany" accompanya_eid="e2" accompanyb_eid="e3"/>
    <eRelation relType="Follow" bevent_eid="e4" aevent_eid="e5"/>
    <eRelation relType="Accompany" accompanya_eid="e5" accompanyb_eid="e6"/>
    <eRelation relType="Follow" bevent_eid="e6" aevent_eid="e7"/>
    <eRelation relType="Accompany" accompanya_eid="e10" accompanyb_eid="e11"/>
    
