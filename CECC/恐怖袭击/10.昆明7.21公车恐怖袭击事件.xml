<?xml version="1.0" encoding="GB2312"?>
<Body>
    <Title>昆明7.21公车恐怖袭击事件</Title>
    <ReportTime type="absTime">2009-7-21 </ReportTime>
    <Content>
        <Paragraph>
            <Sentence>
                <Event eid="e1">
                    <Time tid="t1" type="relTime">今天早上</Time>，在
                    <Location lid="l1">昆明</Location>在上班的高峰时间里面连续发生了两起
                    <Object oid="o1">公共汽车</Object>
                    <Denoter did="d1" type="emergency">爆炸</Denoter>案
                </Event>，截止到目前为止已经
                <Event eid="e2">造成
                    <Participant sid="s2">2</Participant>
                    <Denoter did="d2" type="stateChange">死</Denoter>
                </Event>
                <Event eid="e3">
                    <Participant sid="s3">14</Participant>
                    <Denoter did="d3" type="stateChange">伤</Denoter>
                </Event>。
            </Sentence>
            <Sentence>根据警方的初步判断，这是一起人为的破坏事件，
            <Event eid="e4">
                <Time tid="t4" type="relTime">今天下午</Time>
                <Participant sid="s4">公安部</Participant>已经
                <Denoter did="d4" type="operation">派出</Denoter>了
                <Participant oid="o4" sid="s5,s6">一个刑侦专家组</Participant>
            </Event>
            <Event eid="e5">
                <Denoter did="d5" type="movement">赶赴</Denoter>
                <Location lid="l5" type="destination">当地</Location>
            </Event>，
            <Event eid="e6">协助当地进行
                <Denoter did="d6" type="operation">调查</Denoter>
            </Event>。
        </Sentence>
    </Paragraph>
    </Content>
    <eRelation relType="Causal" cause_eid="e1" effect_eid="e2"/>
    <eRelation relType="Causal" cause_eid="e1" effect_eid="e3"/>
    <eRelation relType="Follow" bevent_eid="e1" aevent_eid="e4"/>
    <eRelation relType="Follow" bevent_eid="e4" aevent_eid="e5"/>
    <eRelation relType="Follow" bevent_eid="e5" aevent_eid="e6"/>
    
    <eAnaphora anaType="Location" aid="l1" antecedent="昆明" rid="l5" anaphor="当地"/>
    <eAnaphora anaType="Time" aid="t0" antecedent="2009-7-21" rid="t1" anaphor="今天早上"/>
    <eAnaphora anaType="Time" aid="t0" antecedent="2009-7-21" rid="t4" anaphor="今天下午"/>
</Body>